{
  "name": "propertypro-lite",
  "version": "1.0.0",
  "description": "Property Pro Lite is a platform where people can create and/or search properties for sale or rent",
  "main": "app.js",
  "scripts": {
    "lint": "eslint .",
    "deploy": "gh-pages -d UI",
    "start": "babel-node server/app.js",
    "start:dev": "nodemon server/app.js --exec babel-node",
    "test": "nyc --reporter=text --reporter=html mocha test/test.js --require @babel/register --exit",
    "coverage": "nyc --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martmaldigital/PropertyPro-lite.git"
  },
  "keywords": [
    "Property",
    "Land",
    "Housing",
    "Sale",
    "Rent",
    "Apartments"
  ],
  "author": "Matthew Aloba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martmaldigital/PropertyPro-lite/issues"
  },
  "homepage": "https://github.com/martmaldigital/PropertyPro-lite#readme",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.4",
    "express": "^4.17.1",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "npm-check-updates": "^3.1.12",
    "nyc": "^14.1.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "nodemon": "^1.19.1"
  }
}
